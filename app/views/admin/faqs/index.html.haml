%p#notice= notice
%h1 Admin Faqs
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @admin_faqs.each do |admin_faq|
      %tr
        %td= link_to 'Show', admin_faq
        %td= link_to t('common.edit'), edit_admin_faq_path(admin_faq)
        %td= link_to t('common.destroy'), admin_faq, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Admin Faq', new_admin_faq_path

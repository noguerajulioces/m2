%p#notice= notice
%h1 Admin Cities
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @admin_cities.each do |admin_city|
      %tr
        %td= link_to 'Show', admin_city
        %td= link_to t('common.edit'), edit_admin_city_path(admin_city)
        %td= link_to t('common.destroy'), admin_city, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Admin City', new_admin_city_path

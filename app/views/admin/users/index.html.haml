%p#notice= notice
%h1 Admin Users
%table
  %thead
    %tr
      %th{:colspan => "3"}
  %tbody
    - @admin_users.each do |admin_user|
      %tr
        %td= link_to 'Show', admin_user
        %td= link_to t('common.edit'), edit_admin_user_path(admin_user)
        %td= link_to t('common.destroy'), admin_user, method: :delete, data: { confirm: 'Are you sure?' }
%br/
= link_to 'New Admin User', new_admin_user_path

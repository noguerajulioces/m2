<p id="notice"><%= notice %></p>

<h1>Admin Amenities</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @admin_amenities.each do |admin_amenity| %>
      <tr>
        <td><%= link_to 'Show', admin_amenity %></td>
        <td><%= link_to 'Edit', edit_admin_amenity_path(admin_amenity) %></td>
        <td><%= link_to 'Destroy', admin_amenity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Admin Amenity', new_admin_amenity_path %>
